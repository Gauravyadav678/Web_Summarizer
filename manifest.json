{
  "manifest_version": 3,
  "name": "Web Summarizer",
  "description": "Summarize the Web Page",
  "version": "1.0",
 
  "icons": {
    "48": "./images/mylogo.png",
    "96": "./images/mylogo.png"
  },
  
 

  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "index.html"
  },
  
  "permissions": ["scripting", "activeTab", "tabs"],
  
  "commands": {
    "_execute_action": {
      "suggested_key": {
        "default": "Ctrl+U",
        "mac": "Command+U"
      }
    }
  }
  
}
